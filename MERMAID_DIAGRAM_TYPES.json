  },
  "commonPatterns": {
    "declaration": "All diagrams start with `<diagramType>` keyword",
    "comments": "Use %% for line comments (all diagrams)",
    "metadata": "Use accTitle and accDescr for accessibility (flowchart, state)",
    "styling": "classDef and class statements (flowchart, state, class)",
    "configuration": "YAML frontmatter with --- delimiters",
    "relationships": "Arrow syntax varies: -->, --, ->, etc.",
    "nodes": "Enclosed in brackets or special characters: [], {}, (), etc.",
    "labels": "Text on nodes or edges: A[Text], A -->|label| B"
  }
}
